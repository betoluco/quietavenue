  "scripts": {
    "start": " react-scripts start",
    "unit-test": "jest",
    "dev": "NODE_ENV=development REACT_APP_DOMAIN=https://d14gjrmy4gfvfp.cloudfront.net CORS=* npm run build  && npm run upload-assets",
    "mirror": "NODE_ENV=production REACT_APP_DOMAIN=https://d14gjrmy4gfvfp.cloudfront.net CORS=* npm run build && npm run clear-assets && npm run upload-assets",
    "prod": "NODE_ENV=production REACT_APP_DOMAIN=https://quietavenue.com CORS=https://quietavenue.com npm run build",
    "build": "rm -rf ./serverBundle.js ./clientBuild ./dist && npm run client-build && npm run server-build",
    "client-build": "webpack --config webpack.client.js",
    "server-build": "webpack --config webpack.server.js",
    "clear-assets": "aws s3 rm s3://quietavenue-dev-s3bucketassets-1k6f7f4u682l1/assets/dist --recursive",
    "upload-assets": "aws s3 cp dist s3://quietavenue-dev-s3bucketassets-1k6f7f4u682l1/assets/dist --recursive"
  },

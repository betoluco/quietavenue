  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.2",
    "aws-sdk": "^2.1080.0",
    "aws-serverless-express": "^3.4.0",
    "axios": "^0.18.0",
    "cors": "^2.8.5",
    "d3-array": "^2.12.1",
    "d3-axis": "^2.1.0",
    "d3-scale": "^3.3.0",
    "d3-selection": "^2.0.0",
    "d3-time": "^2.1.1",
    "d3-time-format": "^3.0.0",
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "serialize-javascript": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development REACT_APP_DOMAIN=https://d2hpvw930r3a95.cloudfront.net npm run build",
    "test": "NODE_ENV=production REACT_APP_DOMAIN=https://d2hpvw930r3a95.cloudfront.net npm run build && npm run clear-assets && npm run upload-assets",
    "prod": "NODE_ENV=production REACT_APP_DOMAIN=https://quietavenue.com npm run build",
    "build": "npm run client-build && npm run server-build",
    "client-build": "webpack --config webpack.client.js",
    "server-build": "webpack --config webpack.server.js",
    "clear-assets": "aws s3 rm s3://quietavenuedev-s3bucketassets-15lz4s5m91ue7/assets/dist --recursive",
    "upload-assets": "aws s3 cp dist s3://quietavenuedev-s3bucketassets-15lz4s5m91ue7/assets/dist --recursive",
    "prod-renew-maps": "npm run clear-old-maps && npm run upload-clientBundle-js-map && npm run upload-main-css-map",
    "clear-old-maps": "aws s3 rm s3://quietavenuerawfiles/quietavenue/mapfiles/ --recursive",
    "upload-clientBundle-js-map": "aws s3 cp clientBuild/clientBundle.js.map  s3://quietavenuerawfiles/quietavenue/mapfiles/",
    "upload-main-css-map": "aws s3 cp clientBuild/main.css.map s3://quietavenuerawfiles/quietavenue/mapfiles/"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "copyfiles": "^2.4.1",
    "core-js": "^3.8.1",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cypress": "^6.9.1",
    "mini-css-extract-plugin": "^2.5.3",
    "postcss": "^8.4.7",
    "postcss-loader": "^5.3.0",
    "postcss-preset-env": "^7.4.1",
    "style-loader": "^2.0.0",
    "tailwindcss": "^3.0.23",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  }
}

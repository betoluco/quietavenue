{
  "name": "quietavenue",
  "version": "1.0.0",
  "description": "react serverside rendering website with hydration",
  "main": "lambda.js",
  "repository": "https://github.com/awslabs/aws-sam-cli/tree/develop/samcli/local/init/templates/cookiecutter-aws-sam-hello-nodejs",
  "author": "betoluco",
  "license": "MIT",
  "dependencies": {
    "aws-serverless-express": "^3.4.0",
    "axios": "^0.18.0",
    "cors": "^2.8.5",
    "d3-axis": "^2.1.0",
    "d3-scale": "^3.3.0",
    "d3-selection": "^2.0.0",
    "d3-time": "^2.1.1",
    "d3-time-format": "^3.0.0",
    "d3-zoom": "^2.0.0",
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development npm run build && npm run upload-assets && npm run remove-old-files",
    "prod": "NODE_ENV=production npm run build && npm run clear-dist-folder && npm run upload-assets && npm run clientBuild && npm run remove-old-files",
    "build": "npm run client-build && npm run server-build"
    "client-build": "webpack --config webpack.client.js",
    "server-build": "webpack --config webpack.server.js",
    "clear-assets": "aws s3 rm s3://quietavenue.com/dist --recursive",
    "upload-assets": "aws s3 cp dist s3://quietavenue.com/dist --recursive",
    "clientBuild": "npm run clear-old-clientBuild && npm run upload-clientBuild",
    "clear-old-clientBuild":"aws s3 rm s3://quietavenue/dist --recursive",
    "upload-clientBuild": "aws s3 cp clientBuild s3://quietavenue/dist --recursive",
    "remove-old-files": "rm -r clientBuild dist",
    "test": "mocha tests/unit/"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.8.1",
    "css-loader": "^5.0.1",
    "cypress": "^6.2.0",
    "mini-css-extract-plugin": "^1.6.0",
    "mocha": "^6.1.4",
    "postcss": "^8.3.0",
    "postcss-loader": "^5.3.0",
    "postcss-preset-env": "^6.7.0",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.1.2",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.7.3"
  }
}
